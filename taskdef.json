{
  "family": "chukka-final-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",

  "executionRoleArn": "arn:aws:iam::287526490745:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::287526490745:role/ecsTaskExecutionRole",

  "containerDefinitions": [
    {
      "name": "chukka-container",
      "image": "287526490745.dkr.ecr.ca-central-1.amazonaws.com/chukka-final-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/chukka-final-task",
          "awslogs-region": "ca-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
